{
	"name": "Default color scheme",
	"groups": {
		"information:unimportant":  { "fg": "vim-mode-bar-innermore-fg", "bg": "vim-mode-bar-innermore-bg", "attrs": [] },
		
		"information:additional":   { "fg": "vim-mode-bar-inner-fg", "bg": "vim-mode-bar-inner-bg", "attrs": [] },
			"file_name":                "information:additional",
				"file_name_no_file":        "file_name",
			"window_title":             "information:additional",
			"attached_clients":         "information:additional",
			"current_tag":              "information:additional",
			"line_percent":             "information:additional",
				"line_percent_gradient":    "line_percent",
			"position":                	"information:additional",
				"position_gradient":        "position",
		
		"background":               { "fg": "vim-mode-bar-innermore-fg", "bg": "vim-mode-bar-innermost-bg", "attrs": [] },
		"background:divider":       { "fg": "vim-mode-bar-innermore-divider-fg", "bg": "vim-mode-bar-innermore-bg", "attrs": [] },
		
		"wal-modified":       		{ "fg": "vim-dark-text", "bg": "wal-yellow", "attrs": [] },
			"modified_indicator":       "wal-modified",
			"branch_dirty":             "wal-modified",
			"modified_buffers":         "wal-modified",
			"file_vcs_status_M":        "wal-modified",
		"paste_indicator":          { "fg": "vim-dark-text", "bg": "wal-cyan", "attrs": ["bold"] },
		"warning":       			{ "fg": "vim-dark-text", "bg": "wal-warning", "attrs": ["bold"] },
			"capslock_indicator":       "warning",
			"readonly_indicator":       "warning",
		"error":                    { "fg": "vim-dark-text", "bg": "wal-red", "attrs": ["bold"] },
		"file_vcs_status":          { "fg": "wal-red", "bg": "vim-mode-bar-inner-bg", "attrs": [] },
		"file_vcs_status_A":        { "fg": "wal-green", "bg": "vim-mode-bar-inner-bg", "attrs": [] },
		
		
		"mode":                     { "fg": "vim-dark-text", "bg": "vim-mode-normal-bg", "attrs": ["bold"] },
		"wal-mode-insert": 			{ "fg": "vim-dark-text", "bg": "vim-mode-insert-bg", "attrs": ["bold"] },
		"wal-mode-visual": 			{ "fg": "vim-dark-text", "bg": "vim-mode-visual-bg", "attrs": ["bold"] },
		"visual_range":             { "fg": "vim-mode-visual-bg", "bg": "vim-mode-bar-inner-bg", "attrs": ["bold"] },
		"wal-mode-replace": 		{ "fg": "vim-dark-text", "bg": "vim-mode-replace-bg", "attrs": ["bold"] },
		
		
		"wal-rightmost_block":      "wal-accent",
			"line_current":             "wal-rightmost_block",
				"line_current_symbol":      "line_current",
				"col_current":              "line_current",
					"virtcol_current": 			"col_current",
						"virtcol_current_gradient": "virtcol_current",
		"branch:divider":           "wal-branch:divider"
	},
	"mode_translations": {
		"i":  {"groups": {"mode": "wal-mode-insert"}},
		"ic": {"groups": {"mode": "wal-mode-insert"}},
		"ix": {"groups": {"mode": "wal-mode-insert"}},
		"v":  {"groups": {"mode": "wal-mode-visual"}},
		"V":  {"groups": {"mode": "wal-mode-visual"}},
		"^V": {"groups": {"mode": "wal-mode-visual"}},
		"R":  {"groups": {"mode": "wal-mode-replace"}},
		"Rc": {"groups": {"mode": "wal-mode-replace"}},
		"Rx": {"groups": {"mode": "wal-mode-replace"}}
	}
}
