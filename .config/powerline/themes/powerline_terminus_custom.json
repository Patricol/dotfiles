{
	"use_non_breaking_spaces": false,
	"dividers": {
		"left": {
			"hard": "█████ ",
			"hard": " ",
			"soft": "████ ",
			"soft": " "
		},
		"right": {
			"hard": " █████",
			"hard": " ",
			"soft": " ████",
			"soft": " "
		}
	},
	"spaces": 1,
	"segment_data": {
		"branch": {
			"before": " ",
			"args": {
				"status_colors": true
			}
		},
		"stash": {
			"before": "ST "
		},
		"cwd": {
			"args": {
				"use_path_separator": false,
				"dir_limit_depth": 16,
				"dir_shorten_len": 16,
				"ellipsis": "┅"
			}
		},
		"line_current_symbol": {
			"contents": ""
		},
		"player": {
			"args": {
				"state_symbols": {
					"fallback": "♫",
					"play":     "▶",
					"pause":    "▮▮",
					"stop":     "■"
				}
			}
		},
		"mode": {
			"args": {
				"override": {
					"viins": "I",
					"vicmd": "C",
					"emacs": "E"
				}
			}
		},
		"continuation": {
			"args": {
				"right_align": true,
				"renames": {
					"quote": "'",
					"dquote": "\""
				}
			}
		},

		"powerline.segments.common.time.date": {
			"args": {
				"format": "%H:%M"
			}
		},

		"powerline.segments.common.net.network_load": {
			"args": {
				"recv_format": "⇓ {value:>8}",
				"sent_format": "⇑ {value:>8}"
			}
		},
		"powerline.segments.common.net.hostname": {
			"before": " ",
			"before": ""
		},
		"powerline.segments.common.bat.battery": {
			"args": {
				"full_heart": "♥",
				"empty_heart": "♥",
				"online": "⚡︎",
				"offline": " "
			}
		},
		"powerline.segments.common.sys.uptime": {
			"before": "↑  "
		},
		"powerline.segments.common.mail.email_imap_alert": {
			"before": "MAIL "
		},
		"powerline.segments.common.env.virtualenv": {
			"before": "(e) "
		},
		"powerline.segments.common.wthr.weather": {
			"args": {
				"icons": {
					"day":           "DAY",
					"blustery":      "WIND",
					"rainy":         "RAIN",
					"cloudy":        "CLOUDS",
					"snowy":         "SNOW",
					"stormy":        "STORM",
					"foggy":         "FOG",
					"sunny":         "SUN",
					"night":         "NIGHT",
					"windy":         "WINDY",
					"not_available": "NA",
					"unknown":       "UKN"
				},
				"temp_format": "{temp:.0f} C"
			}
		},
		"powerline.segments.common.time.fuzzy_time": {
			"args": {
				"unicode_text": true
			}
		},

		"powerline.segments.vim.mode": {
			"args": {
				"override": {
					"n":  "NORMAL",
					"no": "N·OPER",
					"v":  "VISUAL",
					"V":  "V·LINE",
					"^V": "V·BLCK",
					"s":  "SELECT",
					"S":  "S·LINE",
					"^S": "S·BLCK",
					"i":  "INSERT",
					"ic": "I·COMP",
					"ix": "I·C-X ",
					"R":  "RPLACE",
					"Rv": "V·RPLC",
					"Rc": "R·COMP",
					"Rx": "R·C-X ",
					"c":  "COMMND",
					"cv": "VIM·EX",
					"ce": "NRM·EX",
					"r":  "PROMPT",
					"rm": "-MORE-",
					"r?": "CNFIRM",
					"!":  "!SHELL",
					"t":  "TERM  "
				}
			}
		},
		"powerline.segments.vim.visual_range": {
			"args": {
				"CTRL_V_text": "↕{rows} ↔{vcols}",
				"v_text_oneline": "↔{vcols}",
				"v_text_multiline": "↕{rows}",
				"V_text": "⇕{rows}"
			}
		},
		"powerline.segments.vim.file_name": {
			"args": {
				"display_no_file": true,
				"no_file_text": "[No file]"
			}
		},
		"powerline.segments.vim.readonly_indicator": {
			"args": {
				"text": "READ ONLY"
			}
		},
		"powerline.segments.vim.modified_indicator": {
			"args": {
				"text": "+"
			}
		},
		"powerline.segments.vim.tab_modified_indicator": {
			"args": {
				"text": "+"
			}
		},
		"powerline.segments.vim.paste_indicator": {
			"args": {
				"text": "PASTE"
			}
		},
		"powerline.segments.vim.plugin.capslock.capslock_indicator": {
			"args": {
				"text": "CAPS LOCK"
			}
		},
		"powerline.segments.vim.file_vcs_status": {
			"before": " "
		},
		"powerline.segments.vim.virtcol_current": {
			"args": {
				"gradient": false
			},
			"before": ":"
		},
		"powerline.segments.vim.position": {
			"args": {
				"gradient": false
			}
		},
		"powerline.segments.vim.tab": {
			"args": {
				"end": true
			}
		},
		"powerline.segments.vim.bufnr": {
			"after": ":"
		},
		"powerline.segments.vim.tabnr": {
			"after": ":"
		}
	}
}
