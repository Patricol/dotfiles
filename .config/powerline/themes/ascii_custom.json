{
	"use_non_breaking_spaces": false,
	"dividers": {
		"left": {
			"hard": " ",
			"soft": "| "
		},
		"right": {
			"hard": " ",
			"soft": " |"
		}
	},
	"spaces": 1,
	"segment_data": {
		"branch": {
			"before": "BR ",
			"args": {
				"status_colors": true
			}
		},
		"stash": {
			"before": "ST "
		},
		"cwd": {
			"args": {
				"ellipsis": "...",
				"dir_shorten_len": 8
			}
		},
		"line_current_symbol": {
			"contents": "LN"
		},
		"player": {
			"args": {
				"state_symbols": {
					"fallback": "",
					"play":     ">",
					"pause":    "~",
					"stop":     "X"
				}
			}
		},

		"time": {
			"before": ""
		},

		"powerline.segments.common.net.network_load": {
			"args": {
				"recv_format": "DL {value:>8}",
				"sent_format": "UL {value:>8}"
			}
		},
		"powerline.segments.common.net.hostname": {
			"before": ""
		},
		"powerline.segments.common.bat.battery": {
			"args": {
				"full_heart": "O",
				"empty_heart": "O",
				"online": "C",
				"offline": " "
			}
		},
		"powerline.segments.common.sys.uptime": {
			"before": "UP  "
		},
		"powerline.segments.common.mail.email_imap_alert": {
			"before": "MAIL "
		},
		"powerline.segments.common.env.virtualenv": {
			"before": "(e) "
		},
		"powerline.segments.common.wthr.weather": {
			"args": {
				"icons": {
					"day":           "DAY",
					"blustery":      "WIND",
					"rainy":         "RAIN",
					"cloudy":        "CLOUDS",
					"snowy":         "SNOW",
					"stormy":        "STORM",
					"foggy":         "FOG",
					"sunny":         "SUN",
					"night":         "NIGHT",
					"windy":         "WINDY",
					"not_available": "NA",
					"unknown":       "UKN"
				},
				"temp_format": "{temp:.0f} C"
			}
		},
		"powerline.segments.common.time.fuzzy_time": {
			"args": {
				"unicode_text": false
			}
		},

		"powerline.segments.vim.mode": {
			"args": {
				"override": {
					"n":  "NORMAL",
					"no": "N-OPER",
					"v":  "VISUAL",
					"V":  "V-LINE",
					"^V": "V-BLCK",
					"s":  "SELECT",
					"S":  "S-LINE",
					"^S": "S-BLCK",
					"i":  "INSERT",
					"ic": "I-COMP",
					"ix": "I-C_X ",
					"R":  "RPLACE",
					"Rv": "V-RPLC",
					"Rc": "R-COMP",
					"Rx": "R-C_X ",
					"c":  "COMMND",
					"cv": "VIM-EX",
					"ce": "NRM-EX",
					"r":  "PROMPT",
					"rm": "-MORE-",
					"r?": "CNFIRM",
					"!":  "!SHELL",
					"t":  "TERM  "
				}
			}
		},
		"powerline.segments.vim.visual_range": {
			"args": {
				"CTRL_V_text": "{rows} x {vcols}",
				"v_text_oneline": "C:{vcols}",
				"v_text_multiline": "L:{rows}",
				"V_text": "L:{rows}"
			}
		},
		"powerline.segments.vim.file_name": {
			"args": {
				"display_no_file": true,
				"no_file_text": "[No file]"
			}
		},
		"powerline.segments.vim.readonly_indicator": {
			"args": {
				"text": "READ ONLY"
			}
		},
		"powerline.segments.vim.modified_indicator": {
			"args": {
				"text": "+"
			}
		},
		"powerline.segments.vim.tab_modified_indicator": {
			"args": {
				"text": "+"
			}
		},
		"powerline.segments.vim.paste_indicator": {
			"args": {
				"text": "PASTE"
			}
		},
		"powerline.segments.vim.plugin.capslock.capslock_indicator": {
			"args": {
				"text": "CAPS LOCK"
			}
		},
		"powerline.segments.vim.file_vcs_status": {
			"before": " "
		},
		"powerline.segments.vim.virtcol_current": {
			"args": {
				"gradient": false
			},
			"before": ":"
		},
		"powerline.segments.vim.position": {
			"args": {
				"gradient": false
			}
		},
		"powerline.segments.vim.tab": {
			"args": {
				"end": true
			}
		},
		"powerline.segments.vim.bufnr": {
			"after": ":"
		},
		"powerline.segments.vim.tabnr": {
			"after": ":"
		}
	}
}
